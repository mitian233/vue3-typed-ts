{
  "name": "vue3-typed-ts",
  "private": false,
  "version": "1.0.4",
  "type": "module",
  "description": "A Typed.js wrapped library for Vue3 with type support",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "typedjs",
    "vue3",
    "vue",
    "vue3-typed-js",
    "vue3-typedjs",
    "vue-typed-js",
    "vue-typedjs",
    "typescript",
    "vue-typed-ts",
    "vue-typedts"
  ],
  "author": "Mikan Harada",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "dts": "tsc -p ./tsconfig.types.json",
    "preview": "vite preview"
  },
  "dependencies": {
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.9",
    "@vitejs/plugin-vue": "^4.5.2",
    "sass": "^1.70.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.25"
  },
  "publishConfig": {
    "@mitian233:vue3-typed-ts": "https://npm.pkg.github.com"
  }
}
